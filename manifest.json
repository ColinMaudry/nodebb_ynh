{
    "name": "NodeBB",
    "id": "nodebb",
    "packaging_format": 1,
    "description": {
        "en": "Forum software built for the modern web",
        "fr": "Logiciel de forum conÃ§u pour le Web moderne"
    },
    "version": "2.2.0~ynh1",
    "url": "https://nodebb.org",
    "upstream": {
        "license": "GPL-3.0",
        "website": "https://nodebb.org",
        "demo": "https://try.nodebb.org",
        "admindoc": "https://docs.nodebb.org",
        "code": "https://github.com/NodeBB/NodeBB"
    },
    "license": "GPL-3.0",
    "maintainer": {
        "name": "frju365",
        "email": "abld@tutanota.com"
    },
    "requirements": {
        "yunohost": ">= 4.3.0"
    },
    "multi_instance": true,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install": [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/forum",
                "default": "/forum"
            },
            {
                "name": "admin",
                "type": "user"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            },
            {
                "name": "password",
                "type": "password"
            }
        ]
    }
}
